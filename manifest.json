{
  "name": "ELMS STAT",
  "version": "0.0.1",
  "description": "",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "content_scripts": [
  {
    "matches": ["https://umd.instructure.com/", "https://umd.instructure.com/courses/*/grades*"],
    "js": ["dashboard.js", "course.js"],
    "css": ["dashboard.css", "course.css"],
    "all_frames": true
  }
],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/ic_launcher16.png",
      "32": "images/ic_launcher32.png",
      "48": "images/ic_launcher48.png",
      "128": "images/ic_launcher128.png"
    }
  },
  "icons": {
    "16": "images/ic_launcher16.png",
    "32": "images/ic_launcher32.png",
    "48": "images/ic_launcher48.png",
    "128": "images/ic_launcher128.png"
  },
  "web_accessible_resources": ["images/*.png"],
  "manifest_version": 2
}
